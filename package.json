        "@testing-library/react": "^12.1.5",
        "autoprefixer": "^10.4.4",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^27.5.1",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.7.1",
        "eslint": "^8.18.0",
        "eslint-plugin-react": "7.11.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.2.4",
        "jest-date-mock": "^1.0.8",
