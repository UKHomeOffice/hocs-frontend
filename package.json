        "react": "^17.0.2",
        "react-app-polyfill": "^3.0.0",
        "react-dom": "^17.0.2",
        "react-helmet-async": "^1.3.0",
        "react-modal": "^3.11.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.3.0",
        "react-select": "^5.3.2",
        "regenerator-runtime": "^0.13.7",
